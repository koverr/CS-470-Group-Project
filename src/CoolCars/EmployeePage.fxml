      <TableView id="CarsTable" fx:id="CarTable" layoutX="22.0" layoutY="118.0" prefHeight="200.0" prefWidth="628.0">
        <columns>
          <TableColumn id="VINCol" fx:id="VINCol" prefWidth="75.0" text="VIN" />
